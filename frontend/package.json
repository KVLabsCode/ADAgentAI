{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/kill-ports.cjs && concurrently --names \"next,api,agent\" -c \"cyan,yellow,magenta\" \"bun run --bun next dev\" \"cd ../backend && bun --env-file .env --hot api/src/index.ts\" \"node scripts/start-agent.cjs\"",
    "dev:clean": "node scripts/kill-ports.cjs",
    "dev:frontend": "bun run --bun next dev",
    "dev:backend": "cd ../backend && bun --env-file .env --hot api/src/index.ts",
    "dev:agent": "node scripts/start-agent.cjs",
    "build": "bun run tokens:build && bun run --bun next build",
    "start": "bun run --bun next start",
    "lint": "eslint",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "tokens:build": "style-dictionary build --config tokens/style-dictionary.config.mjs",
    "tokens:watch": "style-dictionary build --config tokens/style-dictionary.config.mjs --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "validate:contrast": "bun run scripts/validate-contrast.ts"
  },
